{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "composite": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "jsx": "react-jsx",
        "types": [ "vite/client" ],
        "paths": {
            "@/*": [ "src/renderer/*" ],
            "@assets/*": [ "src/assets/*" ],
            "@shared/*": [ "src/shared/*" ]
        }

    },
    "include": [
        "src/renderer",
        "src/shared",
        "src/assets"
    ]
}